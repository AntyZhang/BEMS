<script>
    var ScrapEqRequestComponent = {
        template: "#ScrapEqRequestComponent",
        data: function () {
            return {
                dataSource: []
            }
        },
        created() {
            //this.LoadAllData();
        },
        methods: {
            LoadAllData() {
                axios.post("/Flow/LoadData", { IsInProgress: this.filter.IsInProgress, NewPage: this.Pagination.CurrentPage }).then((response) => {
                    this.dataSource = response.data;
                    this.Pagination.TotalPage = 25;
                })
            }
        }
    };
</script>